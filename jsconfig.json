{
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "@/components/*": ["components/*"],
      "@/lib/*": ["lib/*"],

      "@/utils/*": ["utils/*"],
      "@/constants/*": ["constants/*"],
      "@/hooks/*": ["hooks/*"],
      "@/middleware/*": ["middleware/*"],
      "@/env*": ["env.js"]
    }
  },
  "exclude": ["node_modules", ".next", "out"]
}
